{
	"author": {
		"email": "jonah@jonah.pw",
		"name": "Jonah Snider",
		"url": "https://jonah.pw"
	},
	"bugs": {
		"url": "https://github.com/pizzafox/renovate-config/issues"
	},
	"devDependencies": {
		"prettier": "2.0.4",
		"semantic-release": "17.0.6"
	},
	"license": "Apache-2.0",
	"main": "./tsc_output/index.js",
	"name": "@pizzafox/renovate-config",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pizzafox/renovate-config.git"
	},
	"scripts": {
		"style": "prettier --check ."
	},
	"renovate-config": {
		"default": {
			"$schema": "https://docs.renovatebot.com/renovate-schema.json",
			"extends": [
				"config:base",
				":semanticCommitTypeAll(build)"
			],
			"labels": [
				"dependencies"
			],
			"packageRules": [
				{
					"automerge": true,
					"updateTypes": [
						"patch"
					]
				}
			],
			"semanticCommits": true
		}
	},
	"version": "0.0.0-development"
}
